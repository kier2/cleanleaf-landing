{
  "version": 2,
  "public": true,
  "rewrites": [
    { "source": "/api/submit", "destination": "/api/submit.js" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
